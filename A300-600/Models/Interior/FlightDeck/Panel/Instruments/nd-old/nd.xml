  <animation>
    <type>texrotate</type>
    <object-name>NDhdg</object-name>
    <property>/orientation/heading-magnetic-deg</property>
    <center>
      <x> 0.5 </x>
      <y> 0.5 </y>
      <z> 0.0 </z>
    </center>
    <axis>
      <x>  0 </x>
      <y>  0 </y>
      <z> -1 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>mfdhdg</object-name>
    <property>/autopilot/internal/fdm-heading-bug-error-deg</property>
    <interpolation>
      <entry><ind> -180 </ind><dep> -47 </dep></entry>
      <entry><ind>  -47 </ind><dep> -47 </dep></entry>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>   47 </ind><dep>  47 </dep></entry>
      <entry><ind>  180 </ind><dep>  47 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m>  0.002 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.047 </z-m>
    </center>
  </animation>

