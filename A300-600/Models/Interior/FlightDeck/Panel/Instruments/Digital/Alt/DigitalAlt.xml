<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>DigitalAlt.ac</path>
	
  <animation>
    <object-name>Bezel</object-name>
    <object-name>BKG</object-name>
    <object-name>Alt1</object-name>
    <object-name>Alt10</object-name>
    <object-name>Alt100</object-name>
    <object-name>Alt1000</object-name>
    <object-name>Alt10000</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>BKG</object-name>
    <object-name>Alt1</object-name>
    <object-name>Alt10</object-name>
    <object-name>Alt100</object-name>
    <object-name>Alt1000</object-name>
    <object-name>Alt10000</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   1.0 </red>
      <green> 0.4 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>

  <!-- Alt -->
  <animation>
    <type>textranslate</type>
    <object-name>Alt10.001</object-name>
    <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.1</factor>
    <step>1</step>
    <scroll>0</scroll>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Alt10</object-name>
   <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.01</factor>
    <step>10</step>
    <scroll>0</scroll>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Alt100</object-name>
   <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.001</factor>
    <step>100</step>
    <scroll>0</scroll>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Alt1000</object-name>
   <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.0001</factor>
    <step>1000</step>
    <scroll>0</scroll>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Alt10000</object-name>
   <property>/autopilot/settings/target-altitude-ft</property>
    <factor>0.00001</factor>
    <step>10000</step>
    <scroll>0</scroll>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>

