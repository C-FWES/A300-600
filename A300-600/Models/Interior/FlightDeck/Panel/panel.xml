<!--AP master-->
   <animation>
        <type>pick</type>
        <object-name>Cylinder.014</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.05</interval-sec>	                      		  
              <binding>
                <command>property-assign</command>
		     <property>autopilot/locks/altitude</property>
			<value type="string"></value>
			<condition>
				<equals>
				<property>autopilot/internal/engaged</property>
				<value>1</value>
				</equals>
			</condition>
	         </binding>
              <binding>
                <command>property-assign</command>
		     <property>autopilot/locks/heading</property>
			<value type="string"></value>
			<condition>
				<equals>
				<property>autopilot/internal/engaged</property>
				<value>1</value>
				</equals>
			</condition>
	         </binding>
            <binding>
                <command>property-assign</command>
		     <property>autopilot/locks/altitude</property>
			<value type="string">vertical-speed-hold</value>
			<condition>
				<equals>
				<property>autopilot/internal/engaged</property>
				<value>0</value>
				</equals>
			</condition>
	         </binding>
              <binding>
                <command>property-assign</command>
		     <property>autopilot/locks/heading</property>
			<value type="string">wing-leveler</value>
			<condition>
				<equals>
				<property>autopilot/internal/engaged</property>
				<value>0</value>
				</equals>
			</condition>
	         </binding>
                  <binding>
		     <property>autopilot/internal/engaged</property>
                        <command>property-cycle</command>
			<value type="double">1</value>
			<value type="double">0</value>
	         </binding>
		</action>

    </animation>


    

