<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel1</object-name>
    
 
    <!-- Objets transparents -->
  </animation>

  <!-- Gear Lever -->
  <animation>
	  <type>rotate</type>
	  <object-name>GearLever</object-name>
	  <property>controls/gear/gear-down</property>
	  <factor>-50</factor>
	  <offset-deg>50</offset-deg>   
	  <axis>  
	   <x1-m>-0.0421</x1-m>
	   <y1-m>0.1625</y1-m>
	   <z1-m>-0.0865</z1-m>
	   <x2-m>-0.0421</x2-m>
	   <y2-m>-0.1625</y2-m>
	   <z2-m>-0.0865</z2-m>
	  </axis>
	</animation>

   <animation>
        <type>pick</type>
        <object-name>GearLever</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.05</interval-sec>
            <binding>
                <command>property-cycle</command>
		     <property>controls/gear/gear-down</property>
			<value>0</value>
			<value>1</value>
            </binding>
	  </action>

    </animation>

 
  <!-- Flaps lever -->
  <animation>
	  <type>rotate</type>
	  <object-name>panelC.027</object-name>
	  <property>controls/flight/flaps</property>
	  <factor>60</factor>
	  <offset-deg>-37</offset-deg>   
	  <axis>  
	   <x1-m>0.004</x1-m>
	   <y1-m>-0.1265</y1-m>
	   <z1-m>0.1000</z1-m>
	   <x2-m>0.004</x2-m>
	   <y2-m>-0.1145</y2-m>
	   <z2-m>0.1000</z2-m>
	  </axis>
	</animation>

  <!-- Park break lever -->
  <animation>
	  <type>rotate</type>
	  <object-name>PbrakeLever</object-name>
	  <property>controls/gear/brake-parking</property>
	  <factor>45</factor>
	  <offset-deg>0</offset-deg>   
	  <axis>  
	   <x1-m>0.1372</x1-m>
	   <y1-m>-0.1445</y1-m>
	   <z1-m>0.1498</z1-m>
	   <x2-m>0.1704</x2-m>
	   <y2-m>-0.1445</y2-m>
	   <z2-m>0.2453</z2-m>
	  </axis>
	</animation>
	<animation>
	  <type>translate</type>
	  <object-name>PbrakeLever</object-name>
	  <property>controls/gear/brake-parking</property>
	  <factor>0.03</factor>
	  <offset-m>-0.03</offset-m>
	  <axis>  
	   <x1-m>0.1372</x1-m>
	   <y1-m>-0.1445</y1-m>
	   <z1-m>0.1498</z1-m>
	   <x2-m>0.1704</x2-m>
	   <y2-m>-0.1445</y2-m>
	   <z2-m>0.2453</z2-m>
	  </axis>
	</animation>

  <!-- Speed brakes lever -->
  <animation>
	  <type>rotate</type>
	  <object-name>SbrakeLever</object-name>
	  <property>controls/flight/spoilers</property>
	  <factor>60</factor>
	  <offset-deg>-45</offset-deg>   
	  <axis>  
	   <x1-m>0.004</x1-m>
	   <y1-m>-0.1265</y1-m>
	   <z1-m>0.1799</z1-m>
	   <x2-m>0.004</x2-m>
	   <y2-m>-0.1145</y2-m>
	   <z2-m>0.1799</z2-m>
	  </axis>
	</animation>

  <!-- Ap master switch-->
  <animation>
	<type>rotate</type>
	  <object-name>Cylinder.014</object-name>
	  <property>autopilot/internal/engaged</property>
         <interpolation>
            <entry><ind>0</ind><dep>-29</dep></entry>
		 <entry><ind>1</ind><dep>29</dep></entry>
         </interpolation>
	  <axis>  
	   <x1-m>0.0699</x1-m>
	   <y1-m>-0.1209</y1-m>
	   <z1-m>0.2023</z1-m>
	   <x2-m>0.0699</x2-m>
	   <y2-m>-0.1374</y2-m>
	   <z2-m>0.2023</z2-m>
	  </axis>
	</animation>

<!--Altitude select knob-->
  <animation>
	<type>rotate</type>
	  <object-name>Cylinder.016</object-name>
	  <property>autopilot/settings/target-altitude-ft</property>
	  <factor>-12</factor>
	  <axis>  
	   <x1-m>0.0977</x1-m>
	   <y1-m>-0.0621</y1-m>
	   <z1-m>0.1860</z1-m>
	   <x2-m>0.0708</x2-m>
	   <y2-m>-0.0621</y2-m>
	   <z2-m>0.1817</z2-m>
	  </axis>
	</animation>
<!--Speed knob-->
  <animation>
	<type>rotate</type>
	  <object-name>Cylinder.019</object-name>
	  <property>autopilot/settings/target-speed-kt</property>
	  <factor>-12</factor>
	  <axis>  
	   <x1-m>0.0974</x1-m>
	   <y1-m>-0.1438</y1-m>
	   <z1-m>0.1871</z1-m>
	   <x2-m>0.0708</x2-m>
	   <y2-m>-0.1438</y2-m>
	   <z2-m>0.1827</z2-m>
	  </axis>
	</animation>

<!--Vertical Speed-->
  <animation>
	<type>rotate</type>
	  <object-name>Cylinder.013</object-name>
	  <property>autopilot/settings/vertical-speed-fpm</property>
	  <factor>-12</factor>
	  <axis>  
	   <x1-m>0.0979</x1-m>
	   <y1-m>0.0774</y1-m>
	   <z1-m>0.1865</z1-m>
	   <x2-m>0.0711</x2-m>
	   <y2-m>0.0774</y2-m>
	   <z2-m>0.1820</z2-m>
	  </axis>
	</animation>
<!--heading knob-->
  <animation>
	<type>rotate</type>
	  <object-name>Cylinder.018</object-name>
	  <property>autopilot/settings/heading-bug-deg</property>
	  <factor>-12</factor>
	  <axis>  
	   <x1-m>0.0974</x1-m>
	   <y1-m>0.0184</y1-m>
	   <z1-m>0.1864</z1-m>
	   <x2-m>0.0706</x2-m>
	   <y2-m>0.0184</y2-m>
	   <z2-m>0.1819</z2-m>
	  </axis>
	</animation>

<!--buttons-->

  <animation>
    <type>material</type>
    <object-name>ATHR.001</object-name>
    <object-name>ATHR</object-name>
    <object-name>Ap-alt-hld</object-name>
    <object-name>Ap-alt-hld.001</object-name>
    <object-name>Hdg-Bttn</object-name>
    <object-name>Hdg-Bttn.001</object-name>
    <object-name>Nav-hold</object-name>
    <object-name>Nav-hold.001</object-name>
    <object-name>V/L</object-name>
    <object-name>V/L.001</object-name>
    <object-name>Apr-hold</object-name>
    <object-name>Apr-hold.001</object-name>
    <object-name>Bezel.000</object-name>
    <object-name>LVCH.001</object-name>
    <object-name>LVCH</object-name>
     <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   1.0 </red>
      <green> 0.4 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>


  <animation>
    <type>select</type>
    <object-name>ATHR</object-name>
	<condition>
		<equals>
             <property>/autopilot/locks/speed</property>
		  <value type="string"></value>
		</equals>
	</condition>
</animation>

  <animation>
    <type>select</type>
    <object-name>ATHR.001</object-name>
	<condition>
		<equals>
             <property>/autopilot/locks/speed</property>
		   <value type="string">speed-with-throttle</value>
		</equals>
	</condition>
</animation>

  <animation>
    <type>select</type>
    <object-name>Ap-alt-hld.001</object-name>
	<condition>
		<equals>
             <property>/autopilot/locks/altitude</property>
		   <value type="string">altitude-hold</value>
		</equals>
	</condition>
</animation>


  <animation>
    <type>select</type>
    <object-name>Hdg-Bttn.001</object-name>
	<condition>
		<equals>
             <property>/autopilot/locks/heading</property>
		   <value type="string">dg-heading-hold</value>
		</equals>
	</condition>
</animation>

  <animation>
    <type>select</type>
    <object-name>Nav-hold.001</object-name>
	<condition>
		<equals>
             <property>/autopilot/locks/heading</property>
		   <value type="string">nav1-hold</value>
		</equals>
	</condition>
</animation>

  <animation>
    <type>select</type>
    <object-name>V/L.001</object-name>
	<condition>
		<equals>
             <property>/autopilot/locks/heading</property>
		   <value type="string">nav1-hold</value>
		</equals>
	</condition>
</animation>

  <animation>
    <type>select</type>
    <object-name>LVCH.001</object-name>
	<condition>
		<or>
            <greater-than>
             <property>/autopilot/internal/target-climb-rate-fps</property>
		   <value>2.0</value>
           </greater-than>
            <less-than>
             <property>/autopilot/internal/target-climb-rate-fps</property>
		   <value>-2.0</value>
           </less-than>
           <equals>
             <property>/autopilot/locks/altitude</property>
		   <value>vertical-speed-hold</value>
		</equals>
	    </or>
	</condition>
</animation>



  <animation>
    <type>select</type>
    <object-name>Apr-hold.001</object-name>
	<condition>
		<equals>
             <property>/autopilot/locks/altitude</property>
		   <value type="string">gs1-hold</value>
		</equals>
	</condition>
</animation>






